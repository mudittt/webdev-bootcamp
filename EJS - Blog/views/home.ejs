<%- include ("partials/header") -%>
    <h1 class="body-header">Home</h1>
    <p class="body-text">
        <%= Content %>
    </p>
    <% posts.forEach(post=> { %>
        <h1 class="body-header">
            <%= post.title %>
        </h1>
        <p class="body-text">
            <%= (post.content).substring(0,100) + "   ..." %>
                <a href="posts/<%= post.title %>">Read more</a>
        </p>
        <% }); %>
            <%- include("partials/footer") -%>